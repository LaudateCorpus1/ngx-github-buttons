{
  "name": "@ctrl/ngx-github-buttons",
  "version": "0.0.0-placeholder",
  "repository": "typectrl/ngx-github-buttons",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "del dist && ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "ghpages": "ng build --prod --no-progress",
    "publish": "cd dist && npm publish --access public",
    "test": "ng test --watch=false --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint ngx-github-buttons --type-check --format stylish",
    "lint:fix": "ng lint ngx-github-buttons --type-check --fix --format stylish",
    "semantic-release": "cd dist && semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "9.1.9",
    "@angular/compiler": "9.1.9",
    "@angular/core": "9.1.9",
    "@angular/forms": "9.1.9",
    "@angular/platform-browser": "9.1.9",
    "@angular/platform-browser-dynamic": "9.1.9",
    "@angular/router": "9.1.9",
    "bootstrap": "4.5.0",
    "core-js": "3.6.5",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.7",
    "@angular/cli": "9.1.7",
    "@angular/compiler-cli": "9.1.9",
    "@angular/language-service": "9.1.9",
    "@types/jasmine": "3.5.10",
    "@types/node": "14.0.10",
    "codelyzer": "5.2.2",
    "del-cli": "3.0.1",
    "jasmine-core": "3.5.0",
    "karma": "5.0.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "9.1.5",
    "semantic-release": "17.0.8",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  },
  "release": {
    "branch": "master"
  }
